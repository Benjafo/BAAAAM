{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "start": "node ./bin/www",
    "typecheck": "tsc --noEmit",
    "test": "NODE_ENV=test jest --runInBand --coverage --detectOpenHandles"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1"
  }
}
